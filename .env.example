# Loam Logger Mobile Environment Variables
# Copy this file to .env and fill in your values

# Expo Router - app directory location
EXPO_ROUTER_APP_ROOT=./app

# API URL - use localhost for development, production URL for releases
EXPO_PUBLIC_API_URL=http://localhost:4000

# Google OAuth Client IDs
# Web Client ID - used for token verification (same as backend GOOGLE_CLIENT_ID)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=

# iOS Client ID - created in Google Cloud Console with bundle ID: com.loamlabs.loamlogger
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
